{
    "manifest_version": 2,
    "name": "Facebook Page ID",
    "version": "1.0",
    "description": "Displays the page name and ID of a Facebook page.",
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "activeTab"
    ]
}
